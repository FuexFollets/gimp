    <config name="ComponentsLigma">
      <value>LIGMA</value>
    </config>
    <!-- Components -->
    <config name="ComponentsLigmaDescription">
      <value>LIGMA and all default plug-ins</value>
    </config>
    <!-- Components -->
    <config name="ComponentsDeps">
      <value>Run-time libraries</value>
    </config>
    <!-- Components -->
    <config name="ComponentsDepsDescription">
      <value>Run-time libraries used by LIGMA, including GTK Run-time Environment</value>
    </config>
    <!-- Components -->
    <config name="ComponentsDebug">
      <value>Debug symbols</value>
    </config>
    <!-- Components -->
    <config name="ComponentsDebugDescription">
      <value>Include information to help with debugging LIGMA</value>
    </config>
    <!-- Components -->
