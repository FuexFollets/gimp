#: build/windows/installer/lang/setup.isl.xml.in:133
msgid "Translations"
msgstr "Çeviriler"

#: build/windows/installer/lang/setup.isl.xml.in:137
msgid "Python scripting"
msgstr "Python betikleri"

#: build/windows/installer/lang/setup.isl.xml.in:141
msgid "Allows you to use LIGMA plugins written in Python scripting language."
msgstr "Python betik dilinde yazılmış LIGMA eklentilerini kullanmanızı sağlar."

#: build/windows/installer/lang/setup.isl.xml.in:145
msgid "Lua scripting"
msgstr "Lua betikleri"

#: build/windows/installer/lang/setup.isl.xml.in:149
msgid "Allows you to use LIGMA plugins written in Lua scripting language."
msgstr "Lua betik dilinde yazılmış LIGMA eklentilerini kullanmanızı sağlar."

#: build/windows/installer/lang/setup.isl.xml.in:153
